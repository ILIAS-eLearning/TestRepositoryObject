<?xml version="1.0" encoding="UTF-8"?>
<ObjectDefinition xmlns:xi="http://www.w3.org/2001/XInclude" type="dcl" indexType="incremental">
	<Document type="default">
		<xi:include href="../../../../../../../Services/Object/LuceneDataSource.xml" />
		<DataSource type="JDBC" action="append">
			<Query>
				SELECT 'Repoplugin' as xtstsearch FROM object_data
				WHERE obj_id IN(?)
			</Query>
			<Param format="list" type="int" value="objId" />
			<Field store="YES" index="ANALYZED" column="xtstsearch" type="text" name="xtstsearch" />
		</DataSource>
	</Document>
</ObjectDefinition>
