<?xml version="1.0"?>
<ObjectDefinition xmlns:xi="http://www.w3.org/2001/XInclude" type="xtst">
	<Document type="default">
		<xi:include href="../../../../../../../Services/Object/LuceneDataSource.xml" />
		<DataSource type="JDBC" action="append">
			<Query>
				SELECT 'Repoplugin' AS xtststring FROM object_data
				WHERE obj_id IN (?)
			</Query>
			<Param format="list" type="int" value="objId" />
			<Field store="YES" index="ANALYZED" column="xtststring" type="text" name="xtststring" />
		</DataSource>
		<DataSource type="JDBC" action="append">
			<Query>
				SELECT 1 as offline FROM rep_robj_xtst_data
				WHERE is_online = 0
				AND id IN(?)
			</Query>
			<Param format="list" type="int" value="objId" />
			<Field store="YES" index="NOT_ANALYZED" column="offline" type="text" name="offline" />
		</DataSource>
	</Document>
</ObjectDefinition>
